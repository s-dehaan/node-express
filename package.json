{
  "name": "node-express",
  "version": "1.0.0",
  "description": "Node Project Template - Express",
  "main": ".src/server.js",
  "scripts": {
    "clean": "npx rimraf node_modules coverage",
    "lint": "prettier --write .",
    "packages:update": "npm update && npx -y npm-check-updates -u",
    "start": "node ./src/server.js",
    "start:devserver": "nodemon --ignore 'tests/**/*.js' -r dotenv/config -L ./src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:single": "jest -- "
  },
  "private": true,
  "keywords": [],
  "author": "Sebastiaan de Haan",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3"
  }
}
